{
  "name": "Solid Structure",
  "description": "DevTool for SolidJS application",
  "version": "1.0.0",
  "manifest_version": 3,
  "devtools_page": "./devtools.html",
  "permissions": [
    "tabs",
    "activeTab",
    "debugger",
    "clipboardRead",
    "clipboardWrite",
    "idle",
    "scripting"
  ],
  "host_permissions": [
    "file:///*",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "icons": {
    "48": "assets/logo48-off.png",
    "128": "assets/logo128-off.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.contentScript.js.7c2698b3.a8357fae.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/utils.4a7bbd0b.js",
        "assets/index.8620d1c0.js",
        "assets/contentScript.js.7c2698b3.js"
      ],
      "use_dynamic_url": true
    }
  ]
}